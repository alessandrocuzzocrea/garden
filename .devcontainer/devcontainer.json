{
	"name": "Alessandro Digital Garden",
	"build": {
	  "dockerfile": "Dockerfile"
	},
	"postCreateCommand": "npm install",
	"forwardPorts": [8080],
	"portsAttributes": {
	  "8080": {
		"label": "Eleventy Server",
		"onAutoForward": "openBrowser"
	  }
	},
	"customizations": {
	  "vscode": {
		"extensions": [
		  "dbaeumer.vscode-eslint",
		  "esbenp.prettier-vscode"
		]
	  }
	}
  }
  
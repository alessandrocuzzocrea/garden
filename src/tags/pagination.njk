---
pagination:
  data: collections
  size: 1
  alias: tag
  filter:
    - all
permalink: /tags/{{ tag }}/
layout: main
eleventyExcludeFromCollections: true
noindex: true
---
<h1>Tagged “{{ tag }}”</h1>
<ul>
{% set taglist = collections[ tag ] %}
{% for note in taglist | reverse %}
  <li><a href="{{ note.url }}">{{ note.data.title }}</a></li>
{% endfor %}
</ul>
